var isPalindrome = function(x) {

   xToString =  x.toString()
    let y=""

    for(let i = xToString.length-1; i>=0; i--)
        {
          y = y + xToString[i]
        }

    console.log(x.toString())
    console.log(y)
    if(x.toString()===y)
    {
        return true
    }
    
    return false

    
}



console.log(isPalindrome(101))